  "description": "Vue 3 frontend for a web-based file manager ",
  "author": "OhYea777 <jack@jwt.nz> (https://jwt.nz)",
  "scripts": {
    "build": "run-p type-check build-only",
    "lint": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts -f ./node_modules/eslint-friendly-formatter . --ignore-path .gitignore",
    "build-only": "vite build",
    "dev": "vite",
    "lint:fix": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts -f ./node_modules/eslint-friendly-formatter . --fix --ignore-path .gitignore",
    "preview": "vite preview --port 4173",
    "start": "vue-cli-service serve --open",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "mdb-vue-ui-kit": "^2.0.0",
    "pinia": "^2.0.21",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.7.14",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-vue": "^9.4.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vue-cli-plugin-mdb5": "^1.2.0",
    "vue-tsc": "^0.40.6"
  },
  "bugs": {
    "url": "https://github.com/OhYea777/dirdir-frontend/issues"
  },
